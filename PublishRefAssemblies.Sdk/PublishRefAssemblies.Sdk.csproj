<Project Sdk="Microsoft.Build.NoTargets/3.5.6">

  <PropertyGroup>
    <CheckEolTargetFramework>false</CheckEolTargetFramework>
    <TargetFramework>net5.0</TargetFramework>
    <PackageReleaseNotes>Removed the need to disable Package Analysis. Now packaging is done that adds the reference assemblies to the autogenerated nuspec used to pack with .NET. Also only imports the Microsoft.NET.Sdk only if it has not already been imported to avoid any possible build errors when importing the Sdk *after* that Sdk was imported.</PackageReleaseNotes>
    <Version>1.0.4</Version>
    <PackageOutputPath>$(MSBuildThisFileDirectory)..\artifacts\</PackageOutputPath>
    <NoPackageAnalysis>true</NoPackageAnalysis>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>Els_kom org.</Company>
    <Authors>Els_kom org.</Authors>
    <PackageProjectUrl>https://github.com/Elskom/PublishRefAssemblies/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Elskom/PublishRefAssemblies/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>PublishRefAssemblies.Sdk</PackageTags>
    <Copyright>Copyright (c) 2021</Copyright>
    <Description>SDK to automatically add Reference Assemblies to your nuget packages.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../Sdk/Sdk.props" Pack="true" PackagePath="Sdk" />
    <None Include="../Sdk/Sdk.targets" Pack="true" PackagePath="Sdk" />
  </ItemGroup>

</Project>
